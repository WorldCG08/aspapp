@model System.Collections.Generic.List<aspapp.Models.Customer>

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (Model.Count == 0)
{
    <text>there are no customers right now.</text>
}
else
{
    <ul>
        @foreach (var customer in Model)
        {
            <li><a href="/customers/details/@customer.Id">@customer.Name</a></li>
        }
    </ul>
}
