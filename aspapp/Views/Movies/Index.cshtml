@model System.Collections.Generic.List<aspapp.Models.Movie>

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.Count == 0)
{
    <text>there are no movies right now.</text>
}
else
{
    <ul>
        @foreach (var movie in Model)
        {
            <li>@movie.Name</li>
        }
    </ul>
}
